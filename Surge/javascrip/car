//获取农历日期
let nowLocalDate =  new Date().toLocaleString('zh-CN-u-ca-chinese').replace(/(\d+)\s*?年/, (_,y)=>"甲乙丙丁戊己庚辛壬癸".charAt((y-4)%10) + "子丑寅卯辰巳午未申酉戌亥".charAt((y-4)%12))
toLocaleDateString('zh-CN-u-ca-chinese', {  year: 'numeric', month: 'long', day: 'numeric' }));
