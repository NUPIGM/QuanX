[URL Rewrite]

# 阻止google.com 跳转到google.com.hk
^https?:\/\/(www\.)?g\.cn https://www.google.com 302
^https?:\/\/(www\.)?google\.cn https://www.google.com 302

# 无指令(谷歌)
^https:\/\/www\.ecosia\.org\/search\?q=([^&]+).+ https://www.google.com/search?q=$1 302

# gg(Google)
^https:\/\/www\.ecosia\.org\/search\?q=gg\+([^&]+) https://www.google.com/search?q=$1 302

# gm (Google图片)
^https:\/\/www\.ecosia\.org\/search\?q=gm\+([^&]+) https://www.google.com/search?&tbm=isch&q=$1 302

# bi (必应)
^https:\/\/www\.ecosia\.org\/search\?q=bi\+([^&]+) https://cn.bing.com/search?q=$1 302

# ddg (DuckDuckGo)
^https:\/\/www\.ecosia\.org\/search\?q=ddg\+([^&]+) https://duckduckgo.com/?ia=about&q=$1 302

# bd(baidu)
^https:\/\/www\.ecosia\.org\/search\?q=bd\+([^&]+) https://m.baidu.com/s?word=$1 302

# == 网盘

# gd(Google Drive)
^https:\/\/www\.ecosia\.org\/search\?q=gd\+([^&]+).+ url 302 https://www.google.com/search?q=%22Google+Drive%22+$1

# tgd(t.me/gdurl 搜索Google Drive)
^https:\/\/www\.ecosia\.org\/search\?q=tgd\+([^&]+).+ url 302 https://t.me/s/gdurl?q=$1

# zgd(zhao.pp.ua搜索Google Drive)
^https:\/\/www\.ecosia\.org\/search\?q=zgd(\+|%20)([^&]+).+ url 302 https://zhao.pp.ua/?q=$2

